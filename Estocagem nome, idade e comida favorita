public class Exemplo{
    String nome;
    int idade;
    String comida;

    public Exemplo(String nome, int idade, String comida){
        this.nome=nome;
        this.idade=idade;
        this.comida=comida;
    }

    public static void main(){
        Exemplo exemplo=new Exemplo("Ighor", 17, "Creme de milho");
        System.out.println("Nome: "+exemplo.nome);
        System.out.println("Idade: "+exemplo.idade);
        System.out.println("Comida favorita: "+exemplo.comida);
    }
}
